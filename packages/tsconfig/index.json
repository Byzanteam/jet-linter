{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "allowJs": false,
    "composite": false,
    "inlineSources": false,
    "lib": ["ESNext", "DOM"],
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "types": ["node", "vite/client"]
  },
  "exclude": ["node_modules"]
}
